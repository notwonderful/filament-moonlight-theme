{
    "name": "notwonderful/filament-moonlight-theme",
    "description": "Moonlight dark theme for Filament PHP - A serene dark theme inspired by JetBrains Moonlight Theme",
    "keywords": [
        "filament",
        "filament-theme",
        "moonlight",
        "dark-theme",
        "theme",
        "colors"
    ],
    "homepage": "https://github.com/notwonderful/filament-moonlight-theme",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "notwonderful",
            "email": "ratatatata322@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "notwonderful\\FilamentMoonlightTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "notwonderful\\FilamentMoonlightTheme\\FilamentMoonlightThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
